{
	"name": "craue/south-park-downloader",
	"type": "project",
	"license": "GPL-3.0",
	"require": {
		"ext-dom": "*",
		"symfony/console": "^3.4",
		"symfony/process": "^3.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0"
	},
	"config": {
		"github-protocols": ["https", "git"],
		"notify-on-install": false,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"platform": {
			"ext-mbstring": "7"
		}
	},
	"autoload": {
		"classmap": ["app/"],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	}
}
